        Title = "WalkSpeed",
        Description = "WS",
        Default = 17,
        Min = 0,
        Max = 500,
        Rounding = 1,
        Callback = function(s)
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
        end
    })

local Slider = Tabs.Player:AddSlider("Slider", {
        Title = "JumpPower",
        Description = "JP",
        Default = 65,
        Min = 0,
        Max = 500,
        Rounding = 1,
        Callback = function(s)
game.Players.LocalPlayer.Character.Humanoid.JumpPower = s

end})
